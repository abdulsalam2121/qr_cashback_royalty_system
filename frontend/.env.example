# Frontend Environment Variables Example
# Copy this file to .env and fill in your actual values

# API Configuration
VITE_API_URL=http://localhost:3001/api

# Stripe Public Key (for frontend payments)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Application Configuration
VITE_APP_NAME=QR Cashback & Loyalty System
VITE_APP_VERSION=1.0.0

# Development/Production Mode
VITE_NODE_ENV=development

# Optional: Google Analytics or other tracking
# VITE_GA_TRACKING_ID=GA-XXXXX-X

# Optional: Error Monitoring (Sentry)
# VITE_SENTRY_DSN=https://your-sentry-dsn-here

# Optional: CDN or Asset URLs
# VITE_ASSETS_URL=https://your-cdn-url.com
